{
    "name": "Colored Colors",
    "version": "0.0.2",
    "manifest_version": 2,
    "description": "Changes the hue of words which name a color.",
    "author": "James Tedesco",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "content.js"
        ]
    }],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "making words better since 2020"
    }
}